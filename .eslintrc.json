{
//  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 5
  },
  "env": {
    "browser": true
  },
//  "plugins": ["react"],
  "rules": {
    "indent": ["error", 2],
    "eqeqeq": "warn",
//    "quotes": ["warn", "double"],
    "semi": ["error", "always"],
    "no-console": "warn"
  }
}
